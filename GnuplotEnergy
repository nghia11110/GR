set view 60,75
                unset key
                set hidden3d
                
                set pm3d
                set palette model RGB
                set palette defined
                set samples 20
                set isosamples 21
                set xlabel "X axis"
                set ylabel "Y axis"
                set zlabel "Z axis" offset 1, 0
                set contour
                set border 4095
                set title "Throughput"
                set dgrid3d 80,80 spline
                splot "DataEnergy" u 1:2:3 w l
